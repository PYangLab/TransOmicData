library(SummarizedExperiment)
library(S4Vectors)
library(here)
load(file=here("data", "C2C12.diff.phos_v2.RData"))
here()
load(file=here("../data", "C2C12.diff.phos_v2.RData"))
load(file=here("data/xiao-muscle/", "C2C12.diff.phos_v2.RData"))
load(file=here("data/xiao-muscle/", "C2C12.diff.proteome_v1.RData"))
load(file=here("data/xiao-muscle/", "C2C12.inhibitors.phospho_v3.RData"))
load(file=here("data/xiao-muscle/", "C2C12.inhibitors.proteome_v3.RData"))
# create metadata file
meta.diff.phospho = DataFrame(time.point = sapply(strsplit(colnames(phospho.pnorm), "_"), "[[",1),
replicate = sapply(strsplit(colnames(phospho.pnorm), "_"), "[[", 2))
meta.diff.prot = DataFrame(time.point = sapply(strsplit(colnames(C2C12.proteome.ruv), "_"), "[[",1),
replicate = sapply(strsplit(colnames(C2C12.proteome.ruv), "_"), "[[", 2))
meta.inhi.phospho = DataFrame(condition = sapply(strsplit(colnames(inhibitors.pnorm), "_"), "[[",1),
replicate = sapply(strsplit(colnames(inhibitors.pnorm), "_"), "[[", 2))
meta.inhi.prot = DataFrame(condition = sapply(strsplit(colnames(inhibitors.proteome.ruv), "_"), "[[",1),
replicate = sapply(strsplit(colnames(inhibitors.proteome.ruv), "_"), "[[", 2))
myogenesis.diff.phosphoproteome.se = SummarizedExperiment(assays=list(ratios = phospho.pnorm),
colData = meta.diff.phospho)
myogenesis.diff.proteome.se = SummarizedExperiment(assays=list(ratios = C2C12.proteome.ruv),
colData = meta.diff.prot)
myogenesis.inhibition.phosphoproteome.se = SummarizedExperiment(assays=list(ratios = inhibitors.pnorm),
colData = meta.inhi.phospho)
myogenesis.inhibition.proteome.se = SummarizedExperiment(assays=list(ratios = inhibitors.proteome.ruv),
colData = meta.inhi.prot)
here("data", "0.99.0", "xiao-muscle", "differentiation.phosphoproteome.rds")
saveRDS(myogenesis.diff.phosphoproteome.se, here("data", "xiao-muscle", "0.99.0", "differentiation.phosphoproteome.rds"))
saveRDS(myogenesis.diff.proteome.se, here("data", "xiao-muscle", "0.99.0", "differentiation.proteome.rds"))
saveRDS(myogenesis.inhibition.phosphoproteome.se, here("data", "xiao-muscle", "0.99.0", "inhibition.phosphoproteome.rds"))
saveRDS(myogenesis.inhibition.proteome.se, here("data", "xiao-muscle", "0.99.0", "inhibition.proteome.rds"))
meta <- data.frame(
Title =  sprintf("Xiao myogensis %s", c("differentaition.phosphoproteome",
"differentiation.proteome",
"inhibition.phosphoproteome",
"inhibition.proteome")),
Description = "timecourse of C2C12 differentiation up to 5 days",
BiocVersion="3.16",
Genome="mm10",
SourceType = "RData",
SourceUrl = "https://proteomecentral.proteomexchange.org/cgi/GetDataset?ID=PXD028713", # GEO accession link or PRIDE link
SourceVersion = "insert naming of individual source file", # in S3 bucket
Species = "Mus musculus",
TaxonomyId=10090,
Coordinate_1_based="NA",
DataProvider="Children's Medical Research Institute, Sydney Australia",
Maintainer="Di Xiao <di.xiao@sydney.edu.au>",
RDataClass= "List",
DispatchClass="Rds",
RDataPath = file.path("TransOmicsData", "xiao-muscle", "0.99.0", c("differentaition.phosphoproteome.rds",
"differentaition.proteome.rds",
"inhibition.phosphoproteome.rds",
"differentaition.proteome.rds")),
stringsAsFactors = FALSE
)
write.csv(meta, file="../extdata/0.99.0/metadata-xiao-muscle.csv", row.names=FALSE)
meta
